    "elementInterval": 5
  },
  "protectedNative": [
    "pass"
  ],
  "encryptedNative": [
    "pass"
  ],
  "objects": [],
  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "Device or service connected",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "info.maintenance",
      "type": "state",
      "common": {
        "role": "indicator.maintenance",
        "name": "Gigaset Elements cloud in maintenance mode",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "info.intrusion",
      "type": "state",
      "common": {
        "role": "indicator.alarm",
        "name": "whether there is an active intrusion alert",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "info.intrusionMode",
      "type": "state",
      "common": {
        "role": "text",
        "name": "configured intrusion mode",
        "type": "string",
        "states": {
          "custom": "Custom",
          "night": "Night",
          "home": "Home",
          "away": "Away"
        },
        "read": true,
        "write": true
      },
      "native": {}
    },
    {
      "_id": "info.userAlarm",
      "type": "state",
      "common": {
        "role": "indicator.alarm",
        "name": "whether there is an active user alarm",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    }
  ]
}
